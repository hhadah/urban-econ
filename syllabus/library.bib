@article{Pickering2000,
abstract = {The premise of current welfare policies is that recipients are avoiding work {\&} a work requirement will end welfare dependency; unemployment is equated with labor market inexperience {\&} economic inactivity. The Pine Ridge Indian Reservation (SD), a poor rural community with high unemployment, contradicts these assumptions. Many Lakota individuals have off-reservation wage work experience; there simply are not enough local jobs to absorb their human capital. Lakota households, however, are involved in a complex combination of socially embedded economic activities outside wage work. Imposing the premises of Temporary Assistance to Needy Families (TANF) on Pine Ridge results in indirect pressures toward urban migration {\&} cultural assimilation. Further, by imposing rigid notions of work, TANF runs the risk of destroying the economic flexibility that makes survival possible for poor households in Pine Ridge. Welfare {\&} development policies need to reflect the real economies of rural American Indian reservations rather than those of superficially assimilated {\&} economically imagined communities. 62 References. Adapted from the source document.},
author = {Pickering, Kathleen},
doi = {10.1111/j.1549-0831.2000.tb00347.x},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Pickering - 2000 - Alternative Economic Strategies in Low-Income Rural Communities TANF, Labor Migration, and the Case of the Pine Ridge.pdf:pdf},
isbn = {0036-0112},
