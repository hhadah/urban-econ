doi = {10.3386/w22487},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Levitt - 2016 - Heads or Tails The Impact of a Coin Toss on Major Life Decisions and Subsequent Happiness.pdf:pdf},
