journal = {Journal of Managerial Psychology},
number = {7/8},
pages = {741--760},
title = {{Age, resistance to change, and job performance}},
url = {http://www.emeraldinsight.com/doi/10.1108/JMP-06-2013-0194},
volume = {28},
year = {2013}
}
