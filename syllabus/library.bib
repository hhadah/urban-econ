issn = {23290390},
journal = {Psychology of Sexual Orientation and Gender Diversity},
keywords = {Affirmative practice,Gender nonconforming,Transgender youth},
number = {2},
pages = {165--172},
title = {{Affirmative practice with transgender and gender nonconforming youth: Expanding the model}},
volume = {3},
year = {2016}
}
