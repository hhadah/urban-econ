@incollection{Dills2010,
abstract = {In this paper we evaluate what economists have learned over the past 40 years about the determinants of crime. We base our evaluation on two kinds of evidence: an examination of aggregate data over long time periods and across countries, and a critical review of the literature. We argue that economists know little about the empirically relevant determinants of crime. Even hypotheses that find some support in U.S. data for recent decades are inconsistent with data over longer horizons or across countries. This conclusion applies both to policy variables like arrest rates or capital punishment and to less conventional factors such as abortion or gun laws. The hypothesis that drug prohibition generates violence, however, is generally consistent with the long times-series and cross-country facts. This analysis is also consistent with a broader perspective in which government policies that affect the nature and amount of dispute resolution play an important role in determining violence.},
author = {Dills, Angela K. and Miron, Jeffrey A. and Summers, Garrett},
booktitle = {The Economics of Crime},
chapter = {8},
editor = {{Di Tella}, Rafael and Edwards, Sebastian and Schargrodsky, Ernesto},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Dills, Miron, Summers - 2010 - What Do Economists Know About Crime.pdf:pdf},
pages = {269--302},
publisher = {National Bureau of Economic Research},
title = {{What Do Economists Know About Crime?}},
year = {2010}
}
@article{Ng2013,
abstract = {One particularly persistent and prevalent negative age stereotype is that older workers are less innovative and more resistant to change. Because older workers are also more likely to have longer organizational tenure, negative age stereotypes contribute to the perception that long-tenured workers are less innovative and more resistant to change, too. Guided by human capital theory, this study argues that the capacity to contribute to innovation-related behaviours (IRB) might actually grow with age and tenure, counteracting the presumed age-related declines in this type of job performance. Using a meta-analysis that included 98 empirical studies, the present research examines the relationships of age and organizational tenure with the generation, dissemination, and implementation of new ideas. Overall, the pattern of results in the study suggests that older workers and longer-tenured workers do not engage in less innovation-related behaviour than their younger and more junior counterparts. In addition, there is little evidence of curvilinearity in the relationships of age and tenure with IRB; workers at the high end of the age and tenure distributions did not perform especially poorly on these tasks. Practitioner points: 1. Contrary to common belief, the results of this study show that age and tenure are not negatively related to innovation-related behaviours. 2. That is, older and longer-tenured workers do not engage in less innovation-related behaviour than younger, more junior workers. 3. These results hold true even at the high end of the age and years-of-service continuum. 3. This study concludes that the negative stereotype that older and longer-tenured workers are less innovative is not based on accumulated empirical evidence. 4. As such, excluding older workers from innovation-related tasks is counterproductive. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
author = {Ng, Thomas W.H. and Feldman, Daniel C.},
doi = {10.1111/joop.12031},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Ng, Feldman - 2013 - A meta-analysis of the relationships of age and tenure with innovation-related behaviour.pdf:pdf},
isbn = {0963-1798},
issn = {09631798},
journal = {Journal of Occupational and Organizational Psychology},
keywords = {Age,Creativity,Innovation,Tenure},
number = {4},
pages = {585--616},
pmid = {90563207},
title = {{A meta-analysis of the relationships of age and tenure with innovation-related behaviour}},
volume = {86},
year = {2013}
}
