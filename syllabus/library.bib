@article{Sarsons2017,
abstract = {This paper provides evidence that a person's gender influences the way others in-terpret information about his or her ability and documents the implications for gender inequality in labor markets. Using data on primary care physicians' (PCPs) referrals to surgeons, I find that PCPs view patient outcomes differently depending on the per-forming surgeon's gender. PCPs become more pessimistic about a female surgeon's ability than a male's after a patient death, indicated by a sharper drop in referrals to the female surgeon. However, PCPs become more optimistic about a male surgeon's ability after a good patient outcome, indicated by a larger increase in the number of referrals the male surgeon receives. PCPs also change their behavior toward other fe-male surgeons after a bad experience with one female surgeon, becoming less likely to refer to new women in the same specialty. There are no such spillovers to other men after a bad experience with one male surgeon. Consistent with learning models, PCPs' reactions to events are strongest when they have just begun to refer to a sur-geon. However, the empirical patterns are consistent with Bayesian learning only if PCPs do not have rational expectations about the true distribution of surgeon ability.},
author = {Sarsons, Heather and Akhtari, Mitra and Barron, Kai and Becker, Anke and Borusyak, Kirill and Chandra, Amitabh and Chassang, Sylvain and Coglianese, John and Danieli, Oren and Dasartha, Krishna and Derenoncourt, Ellora and Fabregas, Raissa and Friedman, John and Fryer, Roland G. Jr. and Ganong, Peter and Glaeser, Edward and Hendren, Nathan and J{\"{a}}ger, Simon and Jena, Anupam and Laird, Jessica and Li, Jing and Libgober, Jonathan and Rabin, Matthew and Schafer, Hannah and Schilbach, Frank and Shah, Nihar and Shleifer, Andrei and Spiess, Jann and Thakral, Neil and Tu, Peter and Vasserman, Shosh and Xu, Chenzi},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Sarsons et al. - 2017 - Interpreting Signals in the Labor Market Evidence from Medical Referrals.pdf:pdf},
