number = {October},
pages = {1580},
title = {{RAND HRS Data Documentation, Version P}},
url = {file:///C:/Users/truefunk/Downloads/randhrs{\_}P.pdf{\%}0Ahttp://hrsonline.isr.umich.edu/modules/meta/rand/randhrso/randhrs{\_}O.pdf},
