journal = {Rural Sociology},
keywords = {BNNA,Sociological Abstracts,To Review},
number = {1},
pages = {148--167},
title = {{Alternative Economic Strategies in Low-Income Rural Communities: TANF, Labor Migration, and the Case of the Pine Ridge Indian Reservation.}},
volume = {65},
