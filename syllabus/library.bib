file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Dupas Alicia Sasser Modestino Muriel Niederle Justin Wolfers et al. - 2021 - Nber Working Paper Series Gender and the Dynamics of Econom.pdf:pdf},
