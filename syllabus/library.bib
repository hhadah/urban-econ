journal = {The Stata Journal},
keywords = {Adjusted predictions,Marginal effects,Margins,Marginsplot,St0260},
number = {2},
pages = {308--331},
title = {{Using the margins command to estimate and interpret adjusted predictions and marginal effects}},
volume = {12},
year = {2012}
}
