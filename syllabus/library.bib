isbn = {1936-2692 (Electronic)},
issn = {15445208},
journal = {Health Affairs},
number = {8},
pages = {1485--1488},
pmid = {18991476},
title = {{Medicaid expansion and state trends in supplemental security income program participation}},
volume = {36},
year = {2017}
}
