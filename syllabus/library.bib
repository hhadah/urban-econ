journal = {Sociological Methods and Research},
keywords = {SIPP,mean-reverting error,measurement error,quantile regression,survey earnings},
number = {1},
pages = {39--72},
title = {{Response Error in Earnings: An Analysis of the Survey of Income and Program Participation Matched With Administrative Data}},
volume = {43},
year = {2014}
}
