doi = {10.1111/socf.12193},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Miller, Grollman - 2015 - The Social Costs of Gender Nonconformity for Transgender Adults Implications for Discrimination and Health.pdf:pdf},
issn = {15737861},
