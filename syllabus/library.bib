@article{\,
abstract = {Contemporary American Indians and Alaska Natives (AIs/ANs) who live in urban areas today face the daunting task of navigating an urban landscape while maintaining the facets of their respective Native cultures. While AIs/ANs continue to grapple with the intergenerational trauma associated with forced assimilation, relocation movements, and boarding schools, these traumas have manifested themselves in elevated rates of psychopathology. AIs/ANs have elevated rates of domestic abuse, poverty, suicide, and substance misuse. Furthermore, AIs/ANs, like many other minority cultures often face discrimination in their everyday lives. In light of the aversive experiences they face, AI/AN people have followed the tenets of ritual and traditional healing to address imbalances in the body, mind, and spirit. For providers working with AI/AN clients, it is important to understand who is using traditional healing and why they are using alternative services. Secondary data analyses of survey data from 389 urban AIs/ANs were utilized in order to determine the relationship between experiences of discrimination and traditional healing use. Analyses indicated that experiences of discrimination in healthcare settings were significantly associated with participation in traditional healing. Analyses also indicated that nearly a quarter of the sample reported discrimination in a healthcare setting, roughly half of the sample had used traditional healing, and that the majority of those who had used traditional healing were women, and ages 35-44 (27 {\%}). This study calls attention to the socio-demographic factors implicated in traditional healing use by urban AI/AN people, in addition to the clinical and demographic characteristics of this sample. {\textcopyright} 2013 Springer Science+Business Media New York.},
author = {Moghaddam, Jacquelene F. and Momper, Sandra L. and Fong, Timothy},
doi = {10.1007/s10900-013-9721-x},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Moghaddam, Momper, Fong - 2013 - Discrimination and participation in traditional healing for American Indians and Alaska natives.pdf:pdf},
issn = {00945145},
journal = {Journal of Community Health},
keywords = {American Indian,Discrimination,Native American,Spiritual healing,Traditional medicine},
number = {6},
pages = {1115--1123},
title = {{Discrimination and participation in traditional healing for American Indians and Alaska natives}},
volume = {38},
year = {2013}
}
@article{Howell2019a,
abstract = {This paper documents American job quality since the late 1970s, surveys leading explanations, and considers the evidence. We focus primarily on wage outcomes for the bottom half of the distribution, beginning with key dimensions of unshared growth in recent decades: the stagnation and decline of real wages and working-age incomes in the bottom-half of the distribution; the rising incidence of low pay, which has been especially pronounced for young workers with less than a college degree; the strong correspondence between wage levels and non-wage benefits and working conditions across four wage contours that range from poverty-wage to good-wage jobs; and recent evidence on changes in the incidence of nonstandard work. We group the main explanations for these developments into three broad visions of how the labor market works: the competitive market model, in which supply and demand for worker skills in competitive external labor markets generates a single market wage by skill group that reflects the worker's marginal product; contested market models, in which wage-setting takes place in imperfectly competitive labor markets in which firms typically have substantial bargaining (monopsony) power; and social-institutional models, which share the contested market models' emphasis on within-firm bargaining over rents but place greater emphasis on the public policies, formal and informal institutions, and the dynamics of workplace cultures and conflict within administered internal labor markets. A review of the explanatory evidence follows, beginning with a critical evaluation of the supply-demand explanation, which has focused on evidence of occupational employment polarization (driven by skill-biased production technologies) and the rise in the college-wage premium. We review the burgeoning research literature that has explored non-skill-based outcomes at the level of establishments, firms and industries. This bargaining power literature is organized into four groups: the increasing market concentration and power of firms, the eroding power of countervailing labor institutions and policies, the acceleration of within-firm employment restructuring (fissuring), and cross-country comparisons. We conclude by summarizing policy recommendations that follow from each of these visions of the post-1979 American labor market.},
author = {Howell, David R. and Kalleberg, Arne L.},
doi = {10.7758/RSF.2019.5.4.01.},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Howell, Kalleberg - 2019 - Declining Job Quality in the United States Explanations and Evidence.pdf:pdf},
keywords = {Bargaining Power},
title = {{Declining Job Quality in the United States: Explanations and Evidence}},
url = {https://equitablegrowth.org/working-papers/declining-job-quality-in-the-united-states-explanations-and-evidence/},
year = {2019}
}
