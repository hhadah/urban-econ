keywords = {applied econometrics,data mining,methodology},
number = {4},
pages = {569--589},
title = {{Sinning in the Basement: What are the Rules? The Ten Commandments of Applied Econometrics}},
url = {http://www.blackwell-synergy.com/links/doi/10.1111/1467-6419.00179},
volume = {16},
year = {2002}
}
