journal = {Social Currents},
keywords = {inequality,mobility,occupations,organizations,poverty,religion,work},
number = {2},
pages = {189--207},
title = {{Religious affiliation and hiring discrimination in the American South: A field experiment}},
url = {http://scu.sagepub.com/lookup/doi/10.1177/2329496514524541},
volume = {1},
