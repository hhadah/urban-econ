doi = {10.1111/j.1464-0597.2007.00317.x},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Chao, Willaby - 2007 - International Employment Discrimination and Implicit Social Cognition New Directions for Theory and Research.pdf:pdf},
