doi = {10.1353/urb.2006.0001},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Baum-Snow, Kahn - 2005 - Effects of Urban Rail Transit Expansions Evidence from Sixteen Cities, 1970-2000.pdf:pdf},
journal = {Brookings-Wharton Papers on Urban Affairs},
