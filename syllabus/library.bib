journal = {Socius},
keywords = {correspondence audit,discrimination,immigration,inequality,race/ethnicity},
pages = {1--16},
title = {{Searching for a Roommate: A Correspondence Audit Examining Racial/Ethnic and Immigrant Discrimination among Millennials}},
