keywords = {Age discrimination,Ageism,Cross-National Studies,Older adults},
number = {6},
pages = {925--933},
pmid = {26224759},
title = {{Greater Perceived Age Discrimination in England than the United States: Results from HRS and ELSA}},
volume = {70},
year = {2015}
}
