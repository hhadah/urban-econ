doi = {10.1177/2041386621992105},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Alcover et al. - 2021 - “Aging-and-Tech Job Vulnerability” A proposed framework on the dual impact of aging and AI, robotics, and automa.pdf:pdf},
