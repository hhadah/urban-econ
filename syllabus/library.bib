isbn = {1111111111},
journal = {Plos One},
number = {6},
pages = {e0267537},
title = {{Do book consumers discriminate against Black, female, or young authors?}},
url = {http://dx.doi.org/10.1371/journal.pone.0267537},
volume = {17},
year = {2022}
}
