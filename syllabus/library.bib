doi = {10.1080/00036840601007351},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop//Brewer, Kelley, Jozefowicz - 2009 - A blueprint for success in the US film industry.pdf:pdf},
isbn = {0003684060100},
issn = {0003-6846},
journal = {Applied Economics},
keywords = {MPPI},
mendeley-tags = {MPPI},
number = {5},
pages = {589--606},
