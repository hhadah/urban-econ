@article{Staples2018,
abstract = {Copyright {\textcopyright} The Society for the Scientific Study of Sexuality. Transgender people are at elevated risk for nonsuicidal self-injury (NSSI) and suicidal ideation compared to the general population. Transgender (trans) refers to a diverse group of people who experience incongruence between their gender identity and sex assigned at birth. The present study is guided by the minority stress model and the psychological mediation framework, which postulate that sexual minority groups experience elevated stress as a result of anti-minority prejudice, contributing to negative mental health outcomes. This study utilized these theories to investigate the role of internalized transnegativity—internalization of negative societal attitudes about one's trans identity—in the relationships of distal trans stress to suicidal ideation and NSSI. A U.S. national sample of trans adults (N = 237) completed a battery of online measures. Structural equation modeling (SEM) was used to compare models with mediation and moderation effects. Results suggested that internalized transnegativity acts as both a mediator and a moderator in the relationship between distal trans stress and suicidal ideation. Log likelihood comparisons suggested moderation models had the superior fit for these data. Results suggest that clinical interventions should directly target individuals' internalized transnegativity as well as societal-level transnegativity.},
author = {Staples, Jennifer M and Neilson, Elizabeth C and Bryan, Amanda E.B. and George, William H},
doi = {10.1080/00224499.2017.1393651},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Staples et al. - 2018 - The Role of Distal Minority Stress and Internalized Transnegativity in Suicidal Ideation and Nonsuicidal Self-In.pdf:pdf},
issn = {15598519},
journal = {Journal of Sex Research},
number = {4-5},
pages = {591--603},
title = {{The Role of Distal Minority Stress and Internalized Transnegativity in Suicidal Ideation and Nonsuicidal Self-Injury Among Transgender Adults}},
volume = {55},
year = {2018}
}
