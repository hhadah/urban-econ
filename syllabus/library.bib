keywords = {Bisexual,Gay,Lesbian,Minority stress,Psychological distress,Transgender youth},
number = {4},
pages = {373--379},
title = {{Minority stress and health: Implications for lesbian, gay, bisexual, transgender, and questioning (LGBTQ) young people}},
volume = {22},
year = {2009}
}
