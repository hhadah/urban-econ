journal = {Labour Economics},
keywords = {Firm behavior,Labor demand,Skills,Unemployment,Vacancies},
pages = {333--347},
publisher = {Elsevier B.V.},
title = {{Downskilling: changes in employer skill requirements over the business cycle}},
url = {http://dx.doi.org/10.1016/j.labeco.2016.05.010},
volume = {41},
year = {2016}
}
