journal = {Counselling and Psychotherapy Research},
keywords = {bisexual,counsellor competency,counsellor education,gay,lesbian,psychotherapy,transgender},
number = {4},
pages = {300--307},
title = {{Addressing disparities: The impact of a lesbian, gay, bisexual, and transgender graduate counselling course}},
