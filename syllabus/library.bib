issn = {2325-8292},
journal = {LGBT Health},
keywords = {african american,health care utilization,lb,lesbian and bisexual women,patient-provider interactions},
number = {2},
pages = {140--146},
pmid = {26790120},
title = {{Predictors and Consequences of Negative Patient-Provider Interactions Among a Sample of African American Sexual Minority Women}},
url = {http://online.liebertpub.com/doi/10.1089/lgbt.2014.0127},
