@article{Breiman1992,
abstract = {When a regression problem contains many predictor variables, it is$\backslash$nrarely wise to try to fit the data by means of a least squares regression$\backslash$non all of the predictor variables. Usually, a regression equation$\backslash$nbased on a few variables will be more accurate and certainly simpler.$\backslash$nThere are various methods for picking "good" subsets of variables,$\backslash$nand programs that do such procedures are part of every widely used$\backslash$nstatistical package. The most common methods are based on stepwise$\backslash$naddition or deletion of variables and on "best subsets." The latter$\backslash$nrefers to a search method that, given the number of variables to$\backslash$nbe in the equation (say, five), locates that regression equation$\backslash$nbased on five variables that has the lowest residual sum of squares$\backslash$namong all five variable equations All of these procedures generate$\backslash$na sequence of regression equations, the first based on one variable,$\backslash$nthe next based on two variables, and so on. Each member of this sequence$\backslash$nis called a submodel and the number of variables in the equation$\backslash$nis the dimensionality of the submodel. A complex problem is determining$\backslash$nwhich submodel of the generated sequence to select. Statistical packages$\backslash$nuse various ad hoc selection methods, including F to enter, F to$\backslash$ndelete, C(p), and t-value cutoffs. Our approach to this problem is$\backslash$nthrough the criterion that a good selection procedure selects dimensionality$\backslash$nso as to give low prediction error (PE), where the PE of a regression$\backslash$nequation is its expected squared error over the points in the X design.$\backslash$nBecause the true PE is unknown, the use of this criteria must be$\backslash$nbased on PE estimates. We introduce a method called the little bootstrap,$\backslash$nwhich gives almost unbiased estimates for submodel PEs and then uses$\backslash$nthese to do submodel selection. Comparison is made to C(p) and other$\backslash$nmethods by analytic examples and simulations. Little bootstrap does$\backslash$nwell-C(p) and, by implication, all selection methods not based on$\backslash$ndata reuse give highly biased results and poor subset selection.},
author = {Breiman, Leo},
doi = {10.1080/01621459.1992.10475276},
issn = {0162-1459},
journal = {Journal of the American Statistical Association},
pages = {738--754},
title = {{The Little Bootstrap and Other Methods for Dimensionality Selection in regression: X-Fixed Prediction Error}},
url = {http://www.tandfonline.com/doi/abs/10.1080/01621459.1992.10475276},
volume = {87},
year = {1992}
}
