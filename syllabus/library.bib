doi = {10.1177/2153368715573608},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Ghoshal - 2015 - What does remembering racial violence do Greensboro's truth commission, mnemonic overlap, and attitudes toward racial r.pdf:pdf},
