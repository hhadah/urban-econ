@article{Skovron2015,
abstract = {We provide a practical guide to the analysis and interpretation of the regression disconti-nuity (RD) design, an empirical strategy increasingly used by political scientists to estimate causal effects with observational data. The defining feature of this design is that treatment as-signment is based on whether the value of a score exceeds a known cutoff. We review core conceptual issues, discussing differences and similarities between RD designs and randomized experiments, basic conditions for identification of RD effects, and parametric and nonparamet-ric alternatives to inference. We distinguish between a continuity-based RD approach, which relies on the continuity of relevant regression functions, and a randomization-based RD ap-proach, which relies on the random assignment of treatment near the cutoff. We discuss best practices for estimation and inference in both cases, and illustrate our recommendations with a political science application. We provide R and Stata code to reproduce the empirical results. * We have benefited from discussions with many people over the years, including Sebasti{\'{a}}n Calonico, Matias Cat-taneo, Bob Erikson,},
author = {Skovron, Christopher and Titiunik, Rocio},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Skovron, Titiunik - 2015 - A Practical Guide to Regression Discontinuity Designs in Political Science.pdf:pdf},
journal = {Working Paper},
