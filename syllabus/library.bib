@article{Tilcsik2011,
abstract = {This article presents the first large-scale audit study of discrimination against openly gay men in the United States. Pairs of fictitious r{\'{e}}sum{\'{e}}s were sent in response to 1,769 job postings in seven states. One r{\'{e}}sum{\'{e}} in each pair was randomly assigned experience in a gay campus organization, and the other r{\'{e}}sum{\'{e}} was assigned a control organization. Two main findings have emerged. First, in some but not all states, there was significant discrimination against the fictitious applicants who appeared to be gay. This geographic variation in the level of discrimination appears to reflect regional differences in attitudes and antidiscrimination laws. Second, employers who emphasized the importance of stereotypically male heterosexual traits were particularly likely to discriminate against openly gay men. Beyond these particular findings, this study advances the audit literature more generally by covering multiple regions and by highlighting how audit techniques may be used to identify stereotypes that affect employment decisions in real labor markets.},
