@book{Kingdon1984,
abstract = {Kingdon addresses the dominant approaches to describing policy change and proposes an alternative, a modified version of the "Garbage Can" approach proposed by Cohen, March, and Olsen. In this approach, government is an "organized anarchy" with three families of processes related to agenda setting: problems, policies, and politics. Actors can be involved in one or several streams at the same time. There are also three major process streams: problem recognition, the formation and refining of policy proposals, and politics. The three process streams are largely independent, but when seperate streams come together (a problem is recognized, a solution is available, the political climate is affable, and constraints are limited) we see significant policy change (i.e. the garbage can). This theory is put in contrast to comprehensive rational decision making (most notably critiqued by Lindblohm) and incrementalism. Though Kingdon sees a place for incrementalism as descriptive, he argues that it doesn't describe big policy change very well, only the incremental maintenance of preexisting policies.},
address = {Boston, MA},
author = {Kingdon, John W. and Thurber, James A.},
publisher = {Little Brown},
title = {{Agendas, alternatives and public policies}},
year = {1984}
}
