isbn = {01621459},
issn = {0162-1459},
journal = {Journal of the American Statistical Association},
keywords = {matching,program evaluation,propensity score},
number = {448},
pages = {1053--1062},
pmid = {12510683},
title = {{Causal effects in nonexperimental studies: Reevaluating the evaluation of training programs}},
url = {http://amstat.tandfonline.com/doi/abs/10.1080/01621459.1999.10473858{\#}.U9Zns1Yq4ds},
