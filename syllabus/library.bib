pages = {104257},
publisher = {Elsevier B.V.},
title = {{Disentangling policy effects using proxy data: Which shutdown policies affected unemployment during the COVID-19 pandemic?}},
url = {https://doi.org/10.1016/j.jpubeco.2020.104257},
volume = {189},
year = {2020}
}
