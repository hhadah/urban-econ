@phdthesis{droesch2002,
abstract = {This paper seeks to determine the underlying causes behind the “runaway film phenomenon”: the mass migration of American feature filmmaking to Canada, particularly British Columbia, which has resulted in the loss of millions of dollars and thousands of motion picture production jobs in the Los Angeles area. Four possible causes are examined: the US/Canada exchange rate; rebates paid by the Canadian national and provincial governments to producers who hire Canadian motion picture production workers; demarcation rules present in the Hollywood production workers' contract; and the escalation of actors' salary demands, which forces producers to cut costs in other areas of filmmaking. Regression and maximum likelihood analyses are used to determine the magnitude of each of these four possible causes on the ratio of runaway to non-runaway films over the time period 1977-2000. The author finds that the demarcation rules, which are accentuated by the rebates and exchange rate, are a driving force behind runaway production. Actors' salaries are also found to promote runaway filming, although to a less determined extent due to data insufficiency.},
author = {Droesch, Audrey},
booktitle = {Stanford University},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop//Droesch - 2010 - Hollywood North The Impact of Costs and Demarcation Rules on the Runaway Film Industry.pdf:pdf},
