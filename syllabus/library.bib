keywords = {Discrimination,Ethnicity,Interethnic competition,Interethnic contact,Labor market,Laboratory experiment},
number = {3},
pages = {731--754},
title = {{Ethnic Discrimination in Recruitment and Decision Makers' Features: Evidence from Laboratory Experiment and Survey Data using a Student Sample}},
volume = {116},
year = {2014}
}
