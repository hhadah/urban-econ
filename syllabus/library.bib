doi = {10.1097/MLR.0000000000000797},
file = {:C$\backslash$:/Users/ButtonPatrickJ/Documents/Mendeley Desktop/Nuti et al. - 2017 - Quality of Care in the United States Territories, 1999-2012.pdf:pdf},
