@article{Nunley2011,
abstract = {We study racial discrimination by simultaneously selling identical products on eBay in pairs of auctions posted under different racially identifying names. We detect significant price differences, which are indicative of in-group biases. White names receive higher prices for distinctively white products, and black names receive higher prices for distinctively black products. But price differences only emerge for sellers who have low eBay feedback scores in less competitive markets. Because the price differences dissipate as sellers accumulate credible reputations, the patterns in the data are indicative of statistical discrimination. Overall, the results suggest that mechanisms designed to reduce informational asymmetries and increased competition are both effective at reducing discrimination in online auctions. {\textcopyright} 2011 Elsevier B.V.},
